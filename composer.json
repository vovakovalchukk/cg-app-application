    "require-dev": {
        "channelgrabber/codeception": "~1.2.1",
        "codeception/codeception": "~1.8.0",
        "phpspec/phpspec": "2.0.*@dev",
        "phpspec/prophecy": "~1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "composer -o dump-autoload",
            "php vendor/zendframework/zendframework/bin/classmap_generator.php -l module -o config/di/modules_classmap.php --overwrite",
            "if [ -d \"vendor/channelgrabber/zf2-v4-ui/public/css\" ]; then rm -rf vendor/channelgrabber/zf2-v4-ui/public/css/*; fi",
            "if [ -d \"public/channelgrabber/settings/css\" ]; then rm -rf public/channelgrabber/settings/css/*; fi",
            "compass compile vendor/channelgrabber/zf2-v4-ui",
            "compass compile public/channelgrabber/settings",
            "cd public-build && node r.js -o require-build.js && cd -"
        ],
        "post-update-cmd": [
            "composer -o dump-autoload",
            "php vendor/zendframework/zendframework/bin/classmap_generator.php -l module -o config/di/modules_classmap.php --overwrite",
            "if [ -d \"vendor/channelgrabber/zf2-v4-ui/public/css\" ]; then rm -rf vendor/channelgrabber/zf2-v4-ui/public/css/*; fi",
            "if [ -d \"public/channelgrabber/settings/css\" ]; then rm -rf public/channelgrabber/settings/css/*; fi",
            "compass compile vendor/channelgrabber/zf2-v4-ui",
            "compass compile public/channelgrabber/settings",
            "cd public-build && node r.js -o require-build.js && cd -"
        ]
    }
}
