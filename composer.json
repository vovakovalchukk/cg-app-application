{
    "repositories": [
        {
            "url": "https://composer.build.channelgrabber.com",
            "type": "composer",
            "options": {
                "ssl": {
                    "local_cert": "composer.pem"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.zendframework.com"
        }
    ],
    "require": {
        "channelgrabber/http": "dev-CGIV-5949-LogReduction as 1.3.15",
        "channelgrabber/stdlib": "~2.12.6",
        "channelgrabber/zf2-stdlib": "~1.1.20",
        "composer/composer": "@dev",
        "guzzle/guzzle": "~3.7",
        "nocarrier/hal": "~0.9.3",
        "zendframework/zend-config": "2.3.*",
        "zendframework/zend-db": "2.3.*",
        "zendframework/zend-di": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*"
    },
    "scripts": {
        "post-install-cmd": [
            "composer -o dump-autoload"
        ],
        "post-update-cmd": [
            "composer -o dump-autoload"
        ]
    }
}