<div class="enable_switch">
    <input id="status-{{id}}" data-id="{{id}}" data-type="{{type}}" class="toggle" type="checkbox" name="active"{{#enabled}} checked="checked"{{/enabled}}>
    <label for="status-{{id}}">
        <span class="toggle_label onlabel">On</span>
        <span class="toggle_label offlabel">Off</span>
    </label>
</div>
