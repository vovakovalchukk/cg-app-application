<div id="paperTypeModule" class="template-module">
    <div class="heading-small">Paper Type</div>
    <div class="paper-type-select">
        {{> elements/custom-select}}
    </div>
    <div class="paper-type-checkbox">
        <div class="std-checkbox">
            <input id="inverseLabelPosition" type="checkbox" value="inverseLabelPosition" {{#selected}} checked="checked"{{/selected}}/>
            <label>
                <span class="checkbox_label">Inverse Label Position</span>
            </label>
        </div>
    </div>
</div>
