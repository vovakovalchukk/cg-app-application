        'ShippingAlias/DomListener/AliasName',
        'ShippingAlias/Mapper'
    ], function(methodCollection, addButtonListener, deleteButtonListener, shippingMethodListener, aliasName, mapper) {
        methodCollection.setItems(mapper.fromCollectionToOptions(shippingMethods));
        addButtonListener.init();
        deleteButtonListener.init();
        shippingMethodListener.init();
        aliasName.init();
    });
});
